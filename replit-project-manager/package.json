{
  "name": "replit-project-manager",
  "version": "1.0.0",
  "description": "A comprehensive git-integrated project management tool with cost-benefit analysis for Dart AI and Replit workflows",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rpm-gitprogress": "./bin/gitprogress.js",
    "rpm-devprogress": "./bin/devprogress.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "gitprogress": "tsx cli/gitprogress.ts",
    "devprogress": "tsx cli/devprogress.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "project-management",
    "git-analysis",
    "cost-benefit-analysis",
    "dart-ai",
    "replit",
    "development-metrics",
    "cocomo",
    "isbsg",
    "dora",
    "progress-reporting",
    "savings-calculation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/replit-project-manager.git"
  },
  "homepage": "https://github.com/yourusername/replit-project-manager#readme",
  "bugs": {
    "url": "https://github.com/yourusername/replit-project-manager/issues"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "dart-tools": "^1.0.0",
    "dotenv": "^16.3.1",
    "simple-git": "^3.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "tsx": "^4.1.2",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/",
    "bin/",
    "benchmarks/",
    "examples/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}